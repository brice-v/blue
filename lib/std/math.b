fun max(x, y) {
    if (x > y) {
        x
    } else {
        y
    }
}

fun min(x, y) {
    if (x < y) {
        x
    } else {
        y
    }
}

fun abs(x) {
    if (x < 0) {
        x * -1
    } else {
        x
    }
}

fun sqrt(x) {
    x ** 0.5
}

fun sum(x) {
    var _sum = 0;
    for (i in x) {
        _sum += i;
    }
    return _sum;
}